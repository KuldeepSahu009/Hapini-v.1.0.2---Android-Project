<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.Dataentryadmin"
    tools:openDrawer="start"
    android:id="@+id/drawerlayout_dataentry">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/blue"
            android:elevation="8dp"
            app:title="DATA ENTRY ADMIN"
            android:id="@+id/dataentryadmintoolbar">
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/framelayoutdataentryadmin"
            android:layout_below="@+id/dataentryadmintoolbar"/>

    </RelativeLayout>
    <com.google.android.material.navigation.NavigationView
        android:layout_width="250dp"
        android:layout_height="match_parent"
        android:id="@+id/dataentryadminnavigationview"
        app:headerLayout="@layout/dataentryadminheadernavigationview_v2"
        app:menu="@menu/dataentryadminmenu"
        app:itemBackground="@color/white"
        app:itemTextColor="@color/navitemdataentry"
        android:layout_gravity="start"/>




</androidx.drawerlayout.widget.DrawerLayout>